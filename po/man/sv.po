#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: data/man/po-translate.1:4
msgid "po-translate - Batch translate PO and TS files using AI"
msgstr ""

#. type: SH
#: data/man/po-translate.1:4
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: data/man/po-translate.1:8
msgid "B<po-translate> [I<OPTIONS>] I<PATHS...>"
msgstr ""

#. type: SH
#: data/man/po-translate.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: data/man/po-translate.1:10
msgid ""
"B<po-translate> batch translates .po, .ts, and .xliff localization files "
"using AI or free translation services. It supports 8 translation backends "
"(Lingva, MyMemory, LibreTranslate, DeepL, Google, OpenAI, Anthropic), custom"
" glossaries, placeholder preservation, and dry-run mode."
msgstr ""

#. type: SH
#: data/man/po-translate.1:10
#, no-wrap
msgid "OPTIONS"
msgstr ""

#. type: TP
#: data/man/po-translate.1:11
#, no-wrap
msgid "I<PATHS>"
msgstr ""

#. type: Plain text
#: data/man/po-translate.1:14
msgid "Files or directories to translate."
msgstr ""

#. type: TP
#: data/man/po-translate.1:14
#, no-wrap
msgid "B<-s>, B<--source> I<LANG>"
msgstr ""

#. type: Plain text
#: data/man/po-translate.1:17
msgid "Source language code (e.g., en). Required."
msgstr ""

#. type: TP
#: data/man/po-translate.1:17
#, no-wrap
msgid "B<-t>, B<--target> I<LANG>"
msgstr ""

#. type: Plain text
#: data/man/po-translate.1:20
msgid "Target language code (e.g., sv). Defaults to system LANG."
msgstr ""

#. type: TP
#: data/man/po-translate.1:20
#, no-wrap
msgid "B<--service> I<SERVICE>"
msgstr ""

#. type: Plain text
#: data/man/po-translate.1:23
msgid ""
"Translation service: lingva, mymemory, libretranslate, deepl, deepl-free, "
"google, openai, anthropic (default: lingva)."
msgstr ""

#. type: TP
#: data/man/po-translate.1:23
#, no-wrap
msgid "B<--api-key> I<KEY>"
msgstr ""

#. type: Plain text
#: data/man/po-translate.1:26
msgid "API key for paid services."
msgstr ""

#. type: TP
#: data/man/po-translate.1:26
#, no-wrap
msgid "B<--url> I<URL>"
msgstr ""

#. type: Plain text
#: data/man/po-translate.1:29
msgid "Custom URL for LibreTranslate."
msgstr ""

#. type: TP
#: data/man/po-translate.1:29
#, no-wrap
msgid "B<--model> I<MODEL>"
msgstr ""

#. type: Plain text
#: data/man/po-translate.1:32
msgid "Model for AI services (e.g., gpt-4o-mini, claude-3-haiku-20240307)."
msgstr ""

#. type: TP
#: data/man/po-translate.1:32
#, no-wrap
msgid "B<--batch-size> I<N>"
msgstr ""

#. type: Plain text
#: data/man/po-translate.1:35
msgid "Entries per API call (default: 10)."
msgstr ""

#. type: TP
#: data/man/po-translate.1:35
#, no-wrap
msgid "B<--glossary> I<FILE>"
msgstr ""

#. type: Plain text
#: data/man/po-translate.1:38
msgid "CSV glossary file (source,target per line) for custom terms."
msgstr ""

#. type: TP
#: data/man/po-translate.1:38
#, no-wrap
msgid "B<--dry-run>"
msgstr ""

#. type: Plain text
#: data/man/po-translate.1:41
msgid "Do not save changes."
msgstr ""

#. type: TP
#: data/man/po-translate.1:41
#, no-wrap
msgid "B<--no-recursive>"
msgstr ""

#. type: Plain text
#: data/man/po-translate.1:44
msgid "Do not search subdirectories."
msgstr ""

#. type: TP
#: data/man/po-translate.1:44
#, no-wrap
msgid "B<-V>, B<--verbose>"
msgstr ""

#. type: Plain text
#: data/man/po-translate.1:47
msgid "Show detailed progress."
msgstr ""

#. type: TP
#: data/man/po-translate.1:47
#, no-wrap
msgid "B<-v>, B<--version>"
msgstr ""

#. type: Plain text
#: data/man/po-translate.1:50
msgid "Show version number and exit."
msgstr ""

#. type: TP
#: data/man/po-translate.1:50
#, no-wrap
msgid "B<--about>"
msgstr ""

#. type: Plain text
#: data/man/po-translate.1:53
msgid "Show application info and exit."
msgstr ""

#. type: SH
#: data/man/po-translate.1:53
#, no-wrap
msgid "EXAMPLES"
msgstr ""

#. type: Plain text
#: data/man/po-translate.1:59
#, no-wrap
msgid ""
"po-translate -s en -t sv messages.po\n"
"po-translate -s en --service deepl --api-key KEY translations/\n"
"po-translate -s en -t de --service openai --model gpt-4o-mini *.po\n"
"po-translate -s en --glossary terms.csv --dry-run project/\n"
